function qam_stream = get_qam_sequence(ofdm_packet)
    [nfft, frame_count] = size(ofdm_packet);
    % Unpack and demodulate ofdm_packet to bitstream
    qam_stream = zeros(frame_count*(nfft/2),1);
    for frame = 1:frame_count
        size(qam_stream(1+(frame-1)*(nfft/2-1):(frame*(nfft/2-1))))
        size(ofdm_packet(frame,2:nfft/2-1)')
        qam_stream(1+(frame-1)*(nfft/2-1):(frame*(nfft/2-1))) = ofdm_packet(frame,2:nfft/2-1)';
    end
end


